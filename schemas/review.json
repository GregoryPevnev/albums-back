{
    "$id": "review",
    "title": "Review",
    "description": "Model for creating reviews",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "rating": {
            "type": "number",
            "minimum": 0,
            "maximum": 10
        },
        "text": {
            "type": "string",
            "default": ""
        }
    },
    "required": ["title", "rating"],
    "additionalProperties": false
}
